<%= form_with model: history, url: admin_finalists_path, local: true do |form| %>

  <div class="row">
    <div class="col-md-4">
      <%= form.label :year, "年" %>
      <%= form.text_field :year, class: "form-control" %>
    </div>
    <div class="col-md-8">
      <%= form.fields_for :comedians, @history.comedians.build do | comedian | %>
        <% (1..5).each do |n| %>
        <h3><%= "#{n}組目" %></h3>
          <%= comedian.collection_select :comedian_id :name, class: "form-control" %>
          <%= comedian.text_field :name, class: "form-control" %>
        <% end %>
      <% end %>
    </div>
  </div>
  <div><%= form.submit "登録する" %></div>
<% end %>